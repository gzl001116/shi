{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/redirect", "dest": "/redirect.html" },
    { "src": "/redirect2", "dest": "/redirect2.html" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}